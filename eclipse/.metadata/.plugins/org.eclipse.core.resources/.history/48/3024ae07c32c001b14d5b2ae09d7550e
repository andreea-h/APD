package multipleProducersMultipleConsumers;

import java.util.concurrent.ArrayBlockingQueue;

public class Buffer {
	ArrayBlockingQueue q;
	
	public Buffer() {
		q = new ArrayBlockingQueue<Integer>();
	}
	
	void put(int value) {
		this.value = value;
	}

	int get() {
		return value;
	}
}
